<section id="cover" class="mobile-only">
    <div id="security_contacts" class="cover-content">
    </div>
</section>

<section id="page_content-with-cover">
    <div class="container-fluid">

        <div class="col-xs-12 col-sm-12 col-md-8 col-md-offset-2" >

            <div class="desktop-medium">
                <h1>Embaixadas / Consulados</h1>
                <hr/>
            </div>

            <div class = "row">
                <div class="col-sm-8 officialContacts">

                    <% contacts.each do |contact| %>
                        <a class="" role="button" data-toggle="collapse" href="#<%= contact[:code] %>" aria-expanded="false" aria-controls="<%= contact[:code] %>">
                            <h4><b> <%= contact[:country] %> <span class="caret"></span></b></h4>
                        </a>
                        <div class="collapse" id="<%= contact[:code] %>">
                            <div class="stuff">
                                <% contact[:contact_info].each do |contact_info| %>
                                    <p><b><%= contact_info[:name] %></b></p>

                                    <% contact_info[:info].each do |info| %>
                                        <p><%= info %></p>
                                    <% end %>
                                    
                                    <br>
                                <% end %>
                            </div>
                        </div>

                    <% end %>

                </div>

                <div class="col-sm-4 desktop-medium" id="sidebar-outer">
                    <div id="sidebar">
                        <img src="/images/security/world.jpeg" class="img-responsive">
                    </div>
                </div>


            </div>
                     
        </div>

    </div>
</section>